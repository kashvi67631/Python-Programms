#include <stdio.h>

int main() {
    /*count zeros , ones and twos */
    int arr[7]={0,0,0,1,1,2,2},one=0,zero=0,two=0;
    int len=sizeof(arr)/sizeof(arr[0]);
    for ( int i=0 ; i<len ; i++){
        if (arr[i]==1)one++;
        else if(arr[i]==0)zero++;
        else {two++;}
    }
    printf("Zero = %d",zero);
    printf("\nOne= %d",one);
    printf("\nTwo= %d",two);
    return 0;
}
